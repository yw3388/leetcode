 if not strs or strs==[""]:
            return ""
        
        
        prefix = strs[0]
        if len(prefix) == 0:
            return ""
        for i in range(len(prefix)):
            c = prefix[i]
            for j in range(1, len(strs)):
                if len(strs[j]) ==0:
                    return ""
                if i == len(strs[j]) or c not in strs[j] or strs[j][i] != c:
                    return strs[0][:i]
        return strs[0]
